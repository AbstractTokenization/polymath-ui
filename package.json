{
  "name": "polymath-ui",
  "version": "0.1.8",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "7.2.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-syntax-async-functions": "^6.1.4",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-plugin-transform-regenerator": "^6.1.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-stage-0": "^6.24.1",
    "css-loader": "^0.28.10",
    "eslint": "4.11.0",
    "eslint-plugin-polymath": "github:PolymathNetwork/eslint-plugin-polymath#master",
    "eslint-plugin-react": "^7.7.0",
    "eslint-watch": "^3.1.3",
    "husky": "0.14.3",
    "lint-staged": "5.0.0",
    "node-sass-chokidar": "^1.1.0",
    "npm-run-all": "^4.1.2",
    "sass-loader": "^6.0.7",
    "style-loader": "^0.20.3"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "babel-runtime": "^6.26.0",
    "carbon-components": "^8.16.8",
    "carbon-components-react": "^5.31.0",
    "carbon-icons": "^5.1.2",
    "prop-types": ">=15",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-transition-group": "^2.2.1"
  },
  "main": "./dist/index.js",
  "scripts": {
    "dev": "npm-run-all -p \"build:js --watch\" \"build:scss --watch\" \"lint -w\"",
    "lint": "esw --fix src",
    "precommit": "lint-staged",
    "build": "npm-run-all -p 'build:*'",
    "build:js": "babel src --out-dir dist",
    "build:scss": "node-sass-chokidar src/ -o dist/",
    "prepublish": "npm run build"
  },
  "license": "UNLICENSED",
  "lint-staged": {
    "src/**/*.js": [
      "eslint --fix",
      "git add"
    ]
  }
}
